type Query {
    users: [User]
    get(id: String): User
}

type Mutation {
    create(username: String!, email: String!, password: String!): User!
    update(input: UserInput!): User!
    delete(id: String!): User!
}

type User {
    id: ID!
    username: String!
    email: String!
    password: String!
    avatar: String
    createdAt: String!
    modifiedAt: String!
}

input UserInput {
    id: ID!
    username: String
    email: String
    password: String
    avatar: String
}